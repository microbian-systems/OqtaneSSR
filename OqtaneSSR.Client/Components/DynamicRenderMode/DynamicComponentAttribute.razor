@using OqtaneSSR.Client.Models

@if (RenderMode == "InteractiveServer")
{
    <InteractiveServer TypeName="@TypeName"></InteractiveServer>
} 
@if (RenderMode == "InteractiveWebAssembly")
{
    <InteractiveWebAssembly TypeName="@TypeName"></InteractiveWebAssembly>
} 
@if (RenderMode == "InteractiveAuto")
{
    <InteractiveAuto TypeName="@TypeName"></InteractiveAuto>
}
@if (RenderMode == "Server")
{
    <DynamicComponent Type="@Type.GetType(TypeName)"></DynamicComponent>
}

@code {
    [CascadingParameter]
    protected PageState PageState { get; set; }

    [CascadingParameter]
    private Module ModuleState { get; set; }

    [Parameter]
    public string TypeName { get; set; }

    [Parameter]
    public string RenderMode { get; set; }
}